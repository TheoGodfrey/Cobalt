{
  "mission_id": "mob_delivery_001",
  "mission_name": "Direct Delivery Test",
  "start_phase": "delivering",
  "hub_failsafe": "RTH",
  "phases": {
    "delivering": {
      "tasks": {
        "payload": {
          "action": "EXECUTE_DELIVERY",
          "actuator": "payload_dropper",
          "strategy": "lawnmower"
        }
      },
      "transitions": {
        "on_state:RETURNING": {
          "payload": "goto:returning"
        }
      }
    },
    "returning": {
      "tasks": {
        "payload": {
          "action": "EXECUTE_RTH",
          "strategy": "lawnmower"
        }
      },
      "transitions": {
        "on_state:LANDING": {
          "payload": "goto:mission_complete"
        }
      }
    },
    "mission_complete": {
      "tasks": {
        "payload": {
          "action": "IGNORE"
        }
      }
    }
  }
}